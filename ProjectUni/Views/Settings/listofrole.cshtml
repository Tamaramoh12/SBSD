
@{
    ViewBag.Title = "List Of Roles";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table" id="r">
    <thead>
        <tr>
            <td>name</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.s)
        {
            <tr>
                <td>
                    @item.Name

                </td>
            </tr>

        }

        </tbody>
    </table>
<script>

    $(document).ready(function () {
        $('#r').DataTable();

    });
</script>


